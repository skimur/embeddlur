@model Embedlur.VideoEmbeddedResult
@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style type="text/css">
        html,body {
            padding: 0;
            margin: 0;
        }
        html {
            height: 100%;
        }
        body {
            overflow: hidden;
            height: 100%;
        }
        iframe {
            width: 100%;
            height: 100%;
        }
    </style>
</head>
<body class="@ViewBag.ProviderName.ToString().ToLower()">
@Html.Raw(Model.Html)
</body>
</html>
